{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/client/**"]
      }
    },
    {
      "src": "dist/client/**",
      "use": "@vercel/static",
      "dest": "dist"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/server/server.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "github": {
    "silent": false
  },
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}